{
  "name": "node-template-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node ./build/bin/index.js",
    "build": "npm run clean && npm run build-server",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel ./src -d ./build -s",
    "run-babel": "babel-node ./src/bin/index.js",
    "start-node": "node src/bin/index.js",
    "debbug": "node --inspect-brk ./src/bin",
    "dev": "nodemon ./src/bin/index.js",
    "run": "nodemon --exec babel-node ./src/bin/index.js",
    "start-build": "node ./build/bin/index.js",
    "start-test": "mocha"
  },
